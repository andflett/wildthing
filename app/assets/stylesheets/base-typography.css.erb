body { 
	font-family: Courier, Helvetica, Arial, sans-serif; 
	color: #576027; 
	-webkit-font-smoothing: antialiased; 
}

/* =h1 */

  h1 { 
    font-size: 26px; 
    font-weight: 100;
		margin-bottom: 26px;
		font-family: Reader;
  }

/* =h2 */

  h2 { 
		font-size: 20px; 
		margin-bottom: 26px;
		font-weight: normal;
  }

/* =h3 */

  h3 { 
    font-size: 16px; 
    margin-bottom: 20px; 
		font-weight: normal;
  }

/* =p */

  p { 
    font-size: 13px;
    line-height: 18px; 
    margin-bottom: 15px;
  }

/* =a */

  a { color: #b6d437; text-decoration: none; }
  a:hover, a:focus, a:active { text-decoration: underline; }

/* =ul,ol,dl */

  ul, dl, ol { padding: 0.25em 2em 1.5em 1em; list-style-position: inside; }
  dl, ul { list-style-type: disc; }
  ol { list-style-type: decimal; }

  ol li ol, ol li ul, ul li ul, ul li ol
  { font-size: 1em; padding: 0.8em 0em 0em 1em; }

/* =li */

  li { font-size: 13px; line-height: 14px; padding-bottom: 4px; }

/* =time */

	time { 
		font-size: 12px; 
		line-height: 12px; 
		display: block; 
		margin-bottom: 16px;
		font-style: italic;
	}
